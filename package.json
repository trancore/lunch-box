{
  "name": "lunch-box",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "build": "pnpm type:check && pnpm build:only && pnpm generate:clasp && cp ./scripts/main.js dist",
    "build:only": "vite-ssg build",
    "build:storybook": "storybook build",
    "dev": "vite",
    "format": "prettier --write src/",
    "generate:clasp": "node ./scripts/generate-clasp-json.ts",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "pull": "clasp pull",
    "push": "cp appsscript.json dist && clasp push",
    "push:all": "pnpm build && pnpm push",
    "storybook": "storybook dev -p 6006 --no-open",
    "type:check": "vue-tsc --build"
  },
  "devDependencies": {
    "@google/clasp": "3.1.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@primeuix/themes": "^1.2.5",
    "@primevue/auto-import-resolver": "^4.4.1",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/vue3-vite": "^10.0.8",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/google-apps-script": "^2.0.7",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vue/compiler-sfc": "^3.5.24",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "^10.0.8",
    "eslint-plugin-vue": "~10.6.0",
    "gas-client": "^1.2.1",
    "jiti": "^2.6.1",
    "leaflet": "^1.9.4",
    "npm-run-all2": "^8.0.4",
    "pinia": "^3.0.4",
    "prettier": "3.6.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "storybook": "^10.0.8",
    "storybook-vue3-router": "^7.0.0",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-ssg": "^28.2.2",
    "vite-svg-loader": "^5.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.5"
  }
}
