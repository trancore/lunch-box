{
  "name": "lunch-box",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "pnpm type:check && pnpm build:only && pnpm generate:clasp && cp ./scripts/main.js dist",
    "build:only": "vite-ssg build",
    "build:storybook": "storybook build",
    "dev": "vite",
    "format": "prettier --write src/",
    "generate:clasp": "node ./scripts/generate-clasp-json.ts",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "pull": "clasp pull",
    "push": "cp appsscript.json dist && clasp push",
    "push:all": "pnpm build && pnpm push",
    "storybook": "storybook dev -p 6006 --no-open",
    "type:check": "vue-tsc --build"
  },
  "devDependencies": {
    "@google/clasp": "3.0.6-alpha",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@primeuix/themes": "^1.2.5",
    "@primevue/auto-import-resolver": "^4.4.1",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/vue3-vite": "^9.1.10",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/google-apps-script": "^2.0.4",
    "@types/node": "^22.18.6",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/compiler-sfc": "^3.5.22",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.33.0",
    "eslint-plugin-storybook": "^9.1.10",
    "eslint-plugin-vue": "~10.4.0",
    "jiti": "^2.5.1",
    "npm-run-all2": "^8.0.4",
    "pinia": "^3.0.3",
    "prettier": "3.6.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "storybook": "^9.1.10",
    "storybook-vue3-router": "^6.0.2",
    "typescript": "~5.9.0",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vite-plugin-singlefile": "^2.3.0",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vite-ssg": "^28.2.1",
    "vite-svg-loader": "^5.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.0"
  }
}
